<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gaotianyu.MINIQQ.Controller.LogInController">
   <children>
      <Button id="login" fx:id="login" layoutX="210.0" layoutY="253.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="32.0" prefWidth="180.0" text="登录" />
      <TextField id="uerid" fx:id="userid" layoutX="173.0" layoutY="142.0" prefHeight="26.0" prefWidth="180.0" promptText="账号" />
      <CheckBox id="reuserid" fx:id="reuserid" layoutX="173.0" layoutY="219.0" mnemonicParsing="false" text="记住账号" />
      <CheckBox id="repwd" fx:id="repwd" layoutX="290.0" layoutY="219.0" mnemonicParsing="false" text="记住密码" />
      <Label id="login" fx:id="signup" layoutX="383.0" layoutY="148.0" onMouseClicked="#onSignUpClick" text="注册账号" />
      <Label id="forget" fx:id="forgetpwd" layoutX="383.0" layoutY="182.0" onMouseClicked="#onForgetClick" text="忘记密码" />
      <PasswordField id="pwd" fx:id="pwd" layoutX="173.0" layoutY="178.0" prefHeight="26.0" prefWidth="180.0" promptText="密码" />
